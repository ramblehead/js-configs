{
  "extends": "../react/tsconfig.json",
  "compilerOptions": {
    // "target": "esnext",
    "target": "es5", // from next.js 13 defaults
    // See https://github.com/microsoft/TypeScript/issues/35822
    // for disabled declarations
    // "moduleResolution": "node",
    // "moduleResolution": "nodenext",
    // "moduleResolution": "node16",
    "moduleResolution": "bundler", // from next.js 13 defaults
    // "traceResolution": true,
    "module": "esnext",
    "noEmit": true,
    "declaration": false,
    "declarationMap": false
  }
}
